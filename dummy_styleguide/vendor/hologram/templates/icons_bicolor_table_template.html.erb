<div class="icons hgt-columns">
  <% examples.each do |example| %>
    <div class="hgt-column one-fifth icon">
      <div class="icon__block">
        <svg width="64" height="64">
          <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/assets/images/front/sprite-bicolor.svg#sprite-bicolor-<%= example.rendered_example %>"></use>
        </svg>
      </div>
      <div class="icon__label"><%= example.rendered_example %></div>
    </div>
  <% end %>
</div>
